generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String     @id @default(uuid())
  name      String
  password  String
  userType  String
  bookmarks Bookmark[]
}

model Bookmark {
  id     String @id @default(uuid())
  user   User   @relation(fields: [userId], references: [id])
  userId String
}

model Video {
  id        Int    @id @default(autoincrement())
  contestId String
  videoURL  String
}
